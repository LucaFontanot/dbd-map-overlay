{
  "name": "dbd-map",
  "version": "1.5.2",
  "description": "Overlay for DBD maps",
  "main": "index.js",
  "author": "Luca <admin@lucaservers.com>",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder -wl",
    "build:win": "electron-builder -w",
    "build:linux": "electron-builder -l",
    "publish": "electron-builder -wl --publish always",
    "publish:win": "electron-builder -w --publish always",
    "publish:linux": "electron-builder -l --publish always"
  },
  "keywords": [],
  "license": "ISC",
  "build": {
    "productName": "Dbd Map Overlay",
    "appId": "com.github.lucafontanot.dbdmapoverlay",
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Games"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "files": [
      "!static/*",
      "!scripts/*",
      "!dist/*",
      "!maps/*",
      "!.github/*",
      "!.vscode/*",
      "!.yarn/*"
    ]
  },
  "devDependencies": {
    "electron": "38.1.0",
    "electron-builder": "26.0.12"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.8.0",
    "bootstrap": "^5.3.1",
    "cron": "^4.1.0",
    "electron-updater": "^6.6.2",
    "image-size": "^1.0.2",
    "jquery": "^3.7.0",
    "marked": "^15.0.7",
    "popper.js": "^1.16.1",
    "uuid": "^11.1.0"
  }
}
